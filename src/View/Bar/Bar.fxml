    <Button fx:id="stop" layoutX="426.0" layoutY="441.0" mnemonicParsing="false" text="stop" />
    <Button fx:id="doubleForward" onAction="#doubleForward" layoutX="469.0" layoutY="441.0" mnemonicParsing="false" text="&gt;&gt;" />
    <Button fx:id="tripleForward" onAction="#tripleForward" layoutX="503.0" layoutY="441.0" mnemonicParsing="false" text="&gt;&gt;&gt;" />
